{
  "name": "DryCat",
  "version": "0.2.0",
  "scripts": {
    "build": "tsc",
    "build:assets": "npx copyfiles -a -u 1 -V -e \"./src/**/*.ts\" \"./src/**/*\" dist",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "rm -rf ./dist && npm run build && npm run build:assets && electron ./dist/main.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "copyfiles": "^2.4.0",
    "electron": "^11.0.3",
    "eslint": "^7.14.0",
    "fs-extra": "^9.0.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {}
}
